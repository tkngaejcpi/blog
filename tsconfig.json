{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],

	"compilerOptions": {
		"baseUrl": "./src",
		"paths": {
			"@config": ["./config.ts"],
			"@content-config": ["./content.config.ts"],
			"@components/*": ["./components/*"],
			"@models/*": ["./models/*"],
			"@utils/*": ["./utils/*"]
		}
	}
}
